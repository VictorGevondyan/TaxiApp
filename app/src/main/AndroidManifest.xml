<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.flycode.paradox.taxiuser">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MenuActivity" />
        <activity android:name=".activities.SettingsActivity" />
        <activity android:name=".activities.HistoryActivity" />
        <activity android:name=".activities.InProgressordersActivity" />
        <activity android:name=".activities.MakeOrderActivity" />
        <activity android:name=".activities.OrderDetailsActivity"></activity>
    </application>

</manifest>
