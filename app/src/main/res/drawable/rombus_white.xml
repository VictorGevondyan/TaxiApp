<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:width="20dp"
        android:height="20dp"
        android:right="4dp"
        android:left="4dp"
        android:top="4dp"
        android:bottom="4dp">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="0" >
            <shape
                android:shape="rectangle">
                <solid
                    android:color="@color/pure_white"/>
            </shape>
        </rotate>
    </item>
</layer-list>