<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:width="150dp"
        android:height="150dp"
        android:right="30dp"
        android:left="30dp"
        android:top="30dp"
        android:bottom="30dp">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="0" >
            <shape
                android:shape="rectangle">
                <solid
                    android:color="@color/cyan_100"/>
            </shape>
        </rotate>
    </item>
</layer-list>
