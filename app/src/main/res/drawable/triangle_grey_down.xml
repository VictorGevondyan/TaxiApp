<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:width="60dp"
        android:height="60dp"
        android:right="20dp"
        android:end="20dp"
        android:left="20dp"
        android:start="20dp"
        android:top="-30dp">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="0" >
            <shape
                android:shape="rectangle">
                <stroke
                    android:color="@color/white_30_opaque"
                    android:width="1dp"/>
            </shape>
        </rotate>
    </item>
    <item
        android:width="84dp"
        android:left="8dp"
        android:start="8dp"
        android:top="-30dp">
        <shape
            android:shape="line">
            <stroke
                android:color="@color/white_30_opaque"
                android:width="1dp"/>
        </shape>
    </item>
</layer-list>